# Script for running automated tests on OSB using Travis-CI, see https://github.com/OpenSourceBrain/osb-model-validation

target: LEMS_SupAxAxFigA2a.xml 
engine: jNeuroML_NEURON

mep: .test.axax_bask.mep
experiments:
  AxAx_or_Basket:
    observables:
      spike times:
        file: 
          path: Sim_Target.CGsupaxax.v.dat
          columns: [0,1]
          scaling: [1, 1]
        spike detection: 
          method: threshold
          threshold: -0.02
        tolerance: 0.007057
