# Script for running automated tests on OSB using Travis-CI, see https://github.com/OpenSourceBrain/osb-model-validation

target: LEMS_FigA3SpinStell333.xml 
engine: jNeuroML_NEURON

mep: .test.spinstellFigA3_333.mep
experiments:
  spinstellFigA3_333:
    observables:
      spike times:
        file: 
          path: Sim_Target.CGspinstell.v.dat
          columns: [0,1]
          scaling: [1, 1]
        spike detection: 
          method: threshold
          threshold: -0.02
        tolerance: 0.02146
